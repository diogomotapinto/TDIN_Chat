<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>
	<system.runtime.remoting>
		<formatter ref="soap" typeFilterLevel="Full" />
		<application name="BEServer">
			<service>
				<wellknown mode="Singleton" type="ServerFram.UserController, ServerFram" objectUri="UserController"/>
			</service>
			<channels>
				<channel ref="tcp server" port="9000" >
					<serverProviders>
						<formatter ref="binary" typeFilterLevel="Full" />
					</serverProviders>
				</channel>
			</channels>
		</application>
	</system.runtime.remoting>
</configuration>