<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>
	<system.runtime.remoting>
		<application name="ServerFram">
			<service>
				<wellknown mode="Singleton" type="UserDatabase.Users, UserDatabase" objectUri="Users"/>
			</service>
			<channels>
				<channel ref="tcp" port="9000" >
					<serverProviders>
						<formatter ref="binary" typeFilterLevel="Full" />
					</serverProviders>
				</channel>
			</channels>
		</application>
	</system.runtime.remoting>
</configuration>